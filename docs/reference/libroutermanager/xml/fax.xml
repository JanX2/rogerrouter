<refsect2 id="fax-status" role="struct">
<title>struct fax_status</title>
<indexterm zone="fax-status"><primary>fax_status</primary></indexterm>
<programlisting language="C">struct fax_status {
	gchar tiff_file[256];
	gchar src_no[64];
	gchar trg_no[64];
	gchar ident[64];
	gchar header[64];
	gchar remote_ident[64];

	enum fax_phase phase;
	gint error_code;
	gboolean sending;
	gchar ecm;
	gchar modem;
	gint bitrate;
	gint encoding;
	gint bad_rows;
	gint page_current;
	gint page_total;
	gint bytes_received;
	gint bytes_sent;
	gint bytes_total;
	gboolean manual_hookup;
	gboolean done;
	gboolean progress_status;

	struct capi_connection *connection;

	fax_state_t *fax_state;
};
</programlisting>
