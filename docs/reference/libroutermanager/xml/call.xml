<refsect2 id="call" role="struct">
<title>struct call</title>
<indexterm zone="call"><primary>call</primary></indexterm>
<programlisting language="C">struct call {
	gint type;
	gchar *date_time;
	gchar *duration;

	struct contact *remote;
	struct contact *local;

	/* Private (e.g. original filename) */
	gchar *priv;
};
</programlisting>
