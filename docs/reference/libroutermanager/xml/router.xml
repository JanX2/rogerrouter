<refsect2 id="router-info" role="struct">
<title>struct router_info</title>
<indexterm zone="router-info"><primary>router_info</primary></indexterm>
<programlisting language="C">struct router_info {
	gchar *host;
	gchar *user;
	gchar *password;
	gchar *name;
	gchar *version;
	gchar *serial;
	gchar *session_id;
	gchar *lang;
	gchar *annex;

	/* Extend */
	gint box_id;
	gint maj_ver_id;
	gint min_ver_id;
	GTimer *session_timer;
};
</programlisting>
