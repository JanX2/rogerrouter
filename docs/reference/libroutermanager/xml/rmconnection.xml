<refsect2 id="RmConnection" role="struct">
<title>RmConnection</title>
<indexterm zone="RmConnection"><primary>RmConnection</primary></indexterm>
<programlisting language="C">typedef struct {
	/* Unique ID */
	guint id;
	/* Type */
	guchar type;
	/* Local number */
	gchar *local_number;
	/* Remote number */
	gchar *remote_number;
	/* Notification widget */
	void *notification;
	/* Private data */
	void *priv;
	/* Connection time */
	GTimer *duration_timer;
} RmConnection;
</programlisting>
</refsect2>

